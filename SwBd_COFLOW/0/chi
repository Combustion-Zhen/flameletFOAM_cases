FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      chi;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 0;

boundaryField
{
    AIR1
    {
        type            calculated;
    }
    AIR2
    {
        type            calculated;
    }
    AIR3
    {
        type            calculated;
    }
    AIR4
    {
        type            calculated;
    }
    AIR
    {
        type            calculated;
    }
    WALL
    {
        type            zeroGradient;
    }
    BOTTOM
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    OUT
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    ATOM
    {
        type            inletOutlet;
        inletValue      uniform 0;
        value           uniform 0;
    }
    FUEL
    {
        type            calculated;
    }
}


